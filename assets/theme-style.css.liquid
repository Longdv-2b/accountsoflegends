.theme-btn {
    display: inline-block;
    font-size: 18px;
    color: #fff;
    border-radius: 4px;
    position: relative;
    z-index: 1;
    background-color: #E93323;
    border-color: #E93323;
    height: 60px;
    line-height: 59px;
    padding: 0 40px;
    overflow: hidden;
    text-align: center;
    text-transform: capitalize;
    font-weight: 500;
}
.theme-btn:hover {
    color: #fff;
    border-color: #313131;
    -webkit-transition: all .5s ease-in-out;
    -moz-transition: all .5s ease-in-out;
    -ms-transition: all .5s ease-in-out;
    -o-transition: all .5s ease-in-out;
    transition: all .5s ease-in-out;
}
.theme-btn::before {
    border-color: #313131;
    background-color: #313131;
    position: absolute;
    left: -1px;
    top: -1px;
    z-index: -1;
    height: 103%;
    width: 102%;
    border-radius: 4px;
    content: "";
    -webkit-transform: scale(0,1);
    -ms-transform: scale(0,1);
    transform: scale(0,1);
    -webkit-transform-origin: top right;
    -ms-transform-origin: top right;
    transform-origin: top right;
    -webkit-transition: transform 500ms cubic-bezier(.86,0,.07,1);
    -moz-transition: transform 500ms cubic-bezier(.86,0,.07,1);
    -o-transition: transform 500ms cubic-bezier(.86,0,.07,1);
    transition: transform 500ms cubic-bezier(.86,0,.07,1);
    transition-timing-function: cubic-bezier(.86,0,.07,1);
    transition-timing-function: cubic-bezier(.86,0,.07,1);
    -webkit-transition-timing-function: cubic-bezier(.86,0,.07,1);
    -moz-transition-timing-function: cubic-bezier(.86,0,.07,1);
    -o-transition-timing-function: cubic-bezier(.86,0,.07,1);
}
.theme-btn:hover::before {
    -webkit-transform: scale(1,1);
    -ms-transform: scale(1,1);
    transform: scale(1,1);
    -webkit-transform-origin: top left;
    -ms-transform-origin: top left;
    transform-origin: top left;
}
