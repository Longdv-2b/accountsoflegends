  text-align: center;
}
.banner-parallax.New-layout .banner-sub-text {
  font-size: 14px;
  color: #fff;
  padding: 0;
  margin-top: 20px;
  margin-bottom: 20px;
}
.banner-parallax.New-layout .banner-heading {
  font-weight: 500;
  padding-top: 0;
  margin-top: 0.7em;
  margin-bottom: 0.6em;
  font-family: "Raleway-Regular";
  text-transform: uppercase;
  font-weight: 900;
}
.banner-parallax.New-layout .container-2 .banner_left .des {
    width: 100%;
}
.banner-parallax.New-layout .container-2 .banner_left .des .parallax-btns.btns_1, .banner-parallax .container-2 .banner_left .des .parallax-btns.btns_2 {
  display: flex;
  justify-content: space-between;
