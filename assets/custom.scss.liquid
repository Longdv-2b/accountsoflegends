.banner-parallax.New-layout a.animated-button:link, a.animated-button:visited {
	position: relative;
	margin: 10px 0 0;
	padding: 15px 20px;
	color: #000d41;
	font-size:14px;
	font-weight: bold;
	text-align: center;
	text-decoration: none;
	text-transform: uppercase;
	overflow: hidden;
	letter-spacing: .08em;
	border-radius: 0;
	text-shadow: 0 0 1px rgba(0, 0, 0, 0.2), 0 1px 0 rgba(0, 0, 0, 0.2);
	-webkit-transition: all 1s ease;
	-moz-transition: all 1s ease;
	-o-transition: all 1s ease;
	transition: all 1s ease;
}
.banner-parallax.New-layout a.animated-button:link:after, a.animated-button:visited:after {
	content: "";
	position: absolute;
	height: 0%;
	left: 50%;
	top: 50%;
	width: 150%;
	z-index: -1;
	-webkit-transition: all 0.75s ease 0s;
	-moz-transition: all 0.75s ease 0s;
	-o-transition: all 0.75s ease 0s;
	transition: all 0.75s ease 0s;
}
.banner-parallax.New-layout a.animated-button:link:hover, a.animated-button:visited:hover {
	color: #FFF;
	text-shadow: none;
}
.banner-parallax.New-layout a.animated-button:link:hover:after, a.animated-button:visited:hover:after {
	height: 350%;
}

.banner-parallax.New-layout a.animated-button.victoria-one {
	border: 2px solid #d0a85c;
}
.banner-parallax.New-layout a.animated-button.victoria-one:after {
	background: #d0a85c;
	-moz-transform: translateX(-50%) translateY(-50%) rotate(-25deg);
	-ms-transform: translateX(-50%) translateY(-50%) rotate(-25deg);
	-webkit-transform: translateX(-50%) translateY(-50%) rotate(-25deg);
	transform: translateX(-50%) translateY(-50%) rotate(-25deg);
}
.banner-parallax.New-layout a.animated-button.victoria-one:hover {
  background-color: #d0a85c !important;
  border-color: #d0a85c !important;
}

@media screen and (max-width: 767px) {
  .banner-parallax.New-layout .container-2 .banner_left {
    width: 100%;
    padding: 50px 0;
  }
  .banner-parallax.New-layout .banner-content .banner_left .parallax-btns .btn-x2 {
